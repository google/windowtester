<?xml version="1.0" encoding="UTF-8"?>
<factories>

	<!-- ================================================================
	This file defines classes implementing WTRuntimeFactory
	and is used when WindowTester is executing outside the OSGi bundle framework
	as in when testing a Swing or stand alone SWT application
	================================================================= -->
	
      <factory os="win32" ws="win32" arch="x86" priority="10"
            class="com.windowtester.runtime.swt.internal.widgets.win32.win32.x86.WTRuntimeFactorySWT_win32_win32_x86"/>
      <factory os="win32" ws="win32" arch="x86_64" priority="10"
            class="com.windowtester.runtime.swt.internal.widgets.win32.win32.x86_64.WTRuntimeFactorySWT_win32_win32_x86_64"/>
      <factory os="linux" ws="gtk" arch="x86" priority="10"
            class="com.windowtester.runtime.swt.internal.widgets.linux.gtk.x86.WTRuntimeFactorySWT_linux_gtk_x86"/>
      <factory os="linux" ws="motif" arch="x86" priority="10"
            class="com.windowtester.runtime.swt.internal.widgets.linux.motif.x86.WTRuntimeFactorySWT_linux_motif_x86"/>
      <factory os="macosx" arch="x86" ws="carbon" priority="10"
            class="com.windowtester.runtime.swt.internal.widgets.carbon.WTRuntimeFactorySWT_carbon_x86"/>
      <factory os="macosx" arch="x86" ws="cocoa" priority="10"
            class="com.windowtester.runtime.swt.internal.widgets.cocoa32.WTRuntimeFactorySWT_cocoa_x86"/>
      <factory os="macosx" arch="x86_64" ws="cocoa" priority="10"
            class="com.windowtester.runtime.swt.internal.widgets.cocoa64.WTRuntimeFactorySWT_cocoa_x86_64"/>

</factories>
